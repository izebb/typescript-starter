{
  "compilerOptions": {
    /* Basic Options */
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "rootDir": "./",
    "removeComments": true,
    "noEmit": true,
    "downlevelIteration": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {},
    "rootDirs": [],
    "importHelpers": true,
    "esModuleInterop": true
  },
  "include": [
    "src"
  ]
}